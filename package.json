{
  "name": "teet",
  "version": "0.1.4",
  "description": "teet static websites from jsx and yaml, no strings attached",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "bin": {
    "teet": "bin/index.js"
  },
  "typings": "esm/index.d.ts",
  "scripts": {
    "predist": "mkdirp esm && rimraf esm/*",
    "dist": "tsc -p src --outDir esm",
    "format": "prettier-standard \"{src,spec}/**/*.[jt]@(s|sx)\"",
    "postdist": "npm run uglify",
    "prepublish": "npm run test",
    "pretest": "npm run dist",
    "test": "tape \"spec/**/*.spec.js\"",
    "uglify": "terser esm/index.js -c -m --toplevel --define process.env.NODE_ENV=\\\"production\\\" -o esm/index.min.js"
  },
  "keywords": [
    "static website generator",
    "jsx",
    "yaml"
  ],
  "author": "Stephane M. Catala <smcatala@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smcatala/teet.git"
  },
  "bugs": {
    "url": "https://github.com/smcatala/teet/issues"
  },
  "homepage": "https://github.com/smcatala/teet#readme",
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "clone-deep": "4.0.1",
    "esm": "3.2.25",
    "findup-sync": "^4.0.0",
    "glob": "7.1.4",
    "glob2base": "0.0.12",
    "js-yaml": "3.13.1",
    "minimist": "1.2.0",
    "mkdirp": "0.5.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rw": "1.3.3"
  },
  "devDependencies": {
    "@types/clone-deep": "^4.0.1",
    "@types/glob": "^7.1.1",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "12.6.9",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.8.5",
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/parser": "1.13.0",
    "http-server": "0.11.1",
    "husky": "3.0.2",
    "lint-staged": "9.2.1",
    "mkdirp": "0.5.1",
    "prettier-standard": "9.1.1",
    "rimraf": "2.6.3",
    "standard": "13.1.0",
    "tape": "4.11.0",
    "terser": "4.1.3",
    "typescript": "3.5.3"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-syntax-dynamic-import"
    ],
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier-standard",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "standard": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ]
  }
}
