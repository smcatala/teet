{
  "name": "teet",
  "version": "0.2.0",
  "description": "teet static websites from jsx and yaml, no strings attached",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "bin": {
    "teet": "bin/index.js"
  },
  "typings": "esm/index.d.ts",
  "scripts": {
    "predist": "mkdirp esm && rimraf esm/*",
    "dist": "tsc -p src --outDir esm",
    "format": "prettier-standard \"{bin,src,spec}/**/*.[jt]@(s|sx)\"",
    "postdist": "npm run uglify",
    "prepublish": "npm run test",
    "pretest": "npm run dist",
    "test": "tape \"spec/**/*.spec.js\"",
    "uglify": "terser esm/index.js -c -m --toplevel --define process.env.NODE_ENV=\\\"production\\\" -o esm/index.min.js"
  },
  "keywords": [
    "static website generator",
    "jsx",
    "yaml"
  ],
  "author": "Stephane M. Catala <smcatala@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smcatala/teet.git"
  },
  "bugs": {
    "url": "https://github.com/smcatala/teet/issues"
  },
  "homepage": "https://github.com/smcatala/teet#readme",
  "dependencies": {
    "@babel/core": "7.6.4",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/preset-env": "7.6.3",
    "@babel/preset-react": "7.6.3",
    "@babel/register": "7.6.2",
    "basic-cursors": "3.0.0",
    "basic-fsa-factories": "2.0.0",
    "chokidar": "3.2.1",
    "esm": "3.2.25",
    "findup-sync": "4.0.0",
    "funky-store": "0.1.0",
    "glob": "7.1.4",
    "glob2base": "0.0.12",
    "js-yaml": "3.13.1",
    "minimist": "1.2.0",
    "mkdirp": "0.5.1",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "rw": "1.3.3"
  },
  "devDependencies": {
    "@types/chokidar": "2.1.3",
    "@types/glob": "7.1.1",
    "@types/mkdirp": "0.5.2",
    "@types/node": "12.7.12",
    "@types/react": "16.9.5",
    "@types/react-dom": "16.9.1",
    "@typescript-eslint/eslint-plugin": "2.3.3",
    "@typescript-eslint/parser": "2.3.3",
    "husky": "3.0.9",
    "lint-staged": "9.4.2",
    "live-server": "1.2.1",
    "mkdirp": "0.5.1",
    "prettier-standard": "15.0.1",
    "rimraf": "3.0.0",
    "standard": "14.3.1",
    "tape": "4.11.0",
    "terser": "4.3.8",
    "typescript": "3.6.4"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-syntax-dynamic-import"
    ],
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier-standard",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "standard": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ]
  }
}
