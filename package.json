{
  "name": "teet",
  "version": "0.0.0",
  "description": "teet static websites from jsx and yaml, no strings attached",
  "main": "dist/index.min.js",
  "bin": "bin/teet",
  "typings": "dist/index.d.ts",
  "scripts": {
    "predist": "mkdirp dist && rimraf dist/*",
    "dist": "tsc -p src --outDir dist",
    "postdist": "npm run uglify",
    "prepublish": "npm run test",
    "pretest": "npm run dist",
    "test": "tape \"spec/**/*.spec.js\"",
    "uglify": "terser dist/index.js -c -m --toplevel --define process.env.NODE_ENV=\\\"production\\\" -o dist/index.min.js"
  },
  "keywords": [
    "static website generator",
    "jsx",
    "yaml"
  ],
  "author": {
    "name": "Stephane M. Catala",
    "email": "stephane@zenyway.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zenyway/teet.git"
  },
  "bugs": {
    "url": "https://github.com/zenyway/teet/issues"
  },
  "homepage": "https://github.com/zenyway/teet#readme",
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
    "@babel/register": "7.4.4",
    "clone-deep": "4.0.1",
    "esm": "3.2.25",
    "glob": "7.1.4",
    "glob2base": "0.0.12",
    "js-yaml": "3.13.1",
    "minimist": "1.2.0",
    "mkdirp": "0.5.1",
    "rw": "1.3.3"
  },
  "devDependencies": {
    "@types/node": "12.6.9",
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/parser": "1.13.0",
    "common-shakeify": "0.6.2",
    "http-server": "0.11.1",
    "husky": "3.0.2",
    "lint-staged": "9.2.1",
    "mkdirp": "0.5.1",
    "prettier-standard": "9.1.1",
    "rimraf": "2.6.3",
    "standard": "13.1.0",
    "tape": "4.11.0",
    "terser": "4.1.3",
    "typescript": "3.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier-standard",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "standard": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ]
  }
}
